<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>GRB_Followup</display-name>
	
	<servlet>
		<servlet-name>RequestServlet</servlet-name>
		<display-name>LT Reactive Time Request Servlet</display-name>
		<servlet-class>ngat.web.RequestServlet</servlet-class>
		<load-on-startup>101</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>RequestServlet</servlet-name>
		<url-pattern>/request_servlet</url-pattern>
	</servlet-mapping>

</web-app>
